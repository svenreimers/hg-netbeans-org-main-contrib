<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">
<filesystem>
  <folder name="Menu">
    <attr name="Build/Versioning" boolvalue="true"/>
    <attr name="Debug/Versioning" boolvalue="true"/>
    <attr name="Versioning/Tools" boolvalue="true"/>
    <folder name="Versioning">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.actions.Bundle" />
      <file name="org-netbeans-modules-vcscore-actions-VcsMountFromTemplateAction.instance" />
      <attr name="org-netbeans-modules-vcscore-actions-VcsMountFromTemplateAction.instance/GlobalCommands" boolvalue="true"/>
      <folder name="GlobalCommands">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.actions.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vcscore/runtime/commandIcon.gif" />
        <file name="org-netbeans-modules-vcscore-actions-VcsGlobalCommandsAction.instance" />
      </folder>
      <attr name="GlobalCommands/org-netbeans-modules-vcscore-actions-VcsCommandsAction.instance" boolvalue="true"/>
      <file name="org-netbeans-modules-vcscore-actions-VcsCommandsAction.instance" /> 
    </folder>
    <folder name="Window">
        <attr name="Debug/Versioning" boolvalue="true"/>
        <attr name="Form/Versioning" boolvalue="true"/>
        <folder name="Versioning">
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:org/openide/resources/actions/empty.gif" />
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.Bundle" />
            <file name="org-netbeans-modules-vcscore-actions-OpenVersioningAction.instance" /> 
            <file name="org-netbeans-modules-vcscore-actions-VcsOutputAction.instance" />
            <attr name="org-netbeans-modules-vcscore-grouping-VcsGroupMenuAction.instance/org-netbeans-modules-vcscore-actions-VcsOutputAction.instance" boolvalue="true"/>          
        </folder>
        <attr name="Versioning/Separator3.instance" boolvalue="true"/>
        <folder name="SelectDocumentNode">
            <file name="org-netbeans-modules-vcscore-actions-VersioningAction.instance" />
            <attr name="SelectInProjectsTabAction.shadow/VersioningAction.instance" boolvalue="true"/>
        </folder>
    </folder>
  </folder>
  
  <folder name="Shortcuts">
      <file name="C-8.instance">
          <attr name="instanceClass" stringvalue="org.netbeans.modules.vcscore.actions.OpenVersioningAction" />
      </file>
      <file name="CS-8.instance">
          <attr name="instanceClass" stringvalue="org.netbeans.modules.vcscore.actions.VersioningAction" />
      </file>
      <file name="C-9.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.vcscore.actions.VcsOutputAction" />
      </file>
  </folder>


  <folder name="Actions">
    <folder name="Versioning">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.actions.Bundle"/>
      <file name="org-netbeans-modules-vcscore-actions-UpdateCommandAction.instance"/>
      <file name="org-netbeans-modules-vcscore-actions-CommitCommandAction.instance"/>
      <file name="org-netbeans-modules-vcscore-actions-HistoryCommandAction.instance"/>
      <file name="org-netbeans-modules-vcscore-actions-DiffCommandAction.instance"/>
      <file name="org-netbeans-modules-vcscore-actions-AddCommandAction.instance"/>
      <file name="org-netbeans-modules-vcscore-actions-RemoveCommandAction.instance"/>
    </folder>
  </folder>

  <folder name="Toolbars">
     <folder name="Versioning">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.Bundle"/>
            <attr boolvalue="true" name="org-netbeans-modules-vcscore-actions-UpdateCommandAction.instance/org-netbeans-modules-vcscore-actions-CommitCommandAction.instance"/>
            <file name="org-netbeans-modules-vcscore-actions-UpdateCommandAction.instance" />
            <attr boolvalue="true" name="org-netbeans-modules-vcscore-actions-CommitCommandAction.instance/org-netbeans-modules-vcscore-actions-AddCommandAction.instance"/>
            <file name="org-netbeans-modules-vcscore-actions-CommitCommandAction.instance" />
            <attr boolvalue="true" name="org-netbeans-modules-vcscore-actions-AddCommandAction.instance/org-netbeans-modules-vcscore-actions-RemoveCommandAction.instance"/>
            <file name="org-netbeans-modules-vcscore-actions-AddCommandAction.instance" />
            <attr boolvalue="true" name="org-netbeans-modules-vcscore-actions-RemoveCommandAction.instance/org-netbeans-modules-vcscore-actions-DiffCommandAction.instance"/>
            <file name="org-netbeans-modules-vcscore-actions-RemoveCommandAction.instance" />
            <attr boolvalue="true" name="org-netbeans-modules-vcscore-actions-DiffCommandAction.instance/org-netbeans-modules-vcscore-actions-HistoryCommandAction.instance"/>
            <file name="org-netbeans-modules-vcscore-actions-DiffCommandAction.instance" />
            <file name="org-netbeans-modules-vcscore-actions-HistoryCommandAction.instance" />
        </folder>
  </folder>

  <folder name="vcs">
    <folder name="Groups">
        <file name="org-netbeans-modules-vcscore-grouping-MainVcsGroupNode.instance" />      
    </folder>
   </folder>
   
  <folder name="Services"> 
    <folder name = "Hidden">
        <file name="org-netbeans-modules-vcscore-settings-GeneralVcsSettings.settings" url="GeneralVcsSettings.settings">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.settings.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vcscore/settings/vcsSettings.gif" />
            <attr name="SystemFileSystem.layer" stringvalue="project" />
        </file>
        <folder name="VersionControl">
            <file name="org-netbeans-modules-vcscore-grouping-VcsGroupSettings.settings" url="VcsGroupSettings.settings">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.grouping.Bundle" />
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vcscore/grouping/MainVcsGroupNodeIcon.gif" />
                <attr name="SystemFileSystem.layer" stringvalue="project" />
            </file>
        </folder>
    </folder>

    <folder name="SearchType">
        <file name="org-netbeans-modules-vcscore-search-VcsSearchType.settings" url="VcsSearchType.settings">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.search.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vcscore/search/find.gif" />
        </file>
    </folder>
  </folder>

  <folder name="UI">
    <folder name="Runtime">
      <file name="org-netbeans-modules-vcscore-runtime-RuntimeMainNode.instance"/>
    </folder>
    <folder name="Services"> 
      <folder name="SourceCreationAndManagement"> 
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.Bundle" />

        <file name="org-netbeans-modules-vcscore-settings-GeneralVcsSettingsNode.instance">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.settings.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/vcscore/settings/vcsSettings.gif" />
        </file>
      </folder>
    </folder>
  </folder>

  <!-- Put all VCS-related mount templates into this folder only -->
  <folder name="VCSMountTemplates">
  </folder>

   <folder name="Templates">
     <folder name="Mount">
      
       <attr name="org-netbeans-core-ExLocalFileSystem.settings/VCS" boolvalue="true" />
       <attr name="org-netbeans-core-ExJarFileSystem.settings/VCS" boolvalue="true" />
      
       <folder name="VCS">
       <!-- Put links to templates in "VCSMountTemplates" here in case VCS mount menu items should be groupped in a sub-menu
            See the mf-layer.xml in vcsgeneric module, as it may need some tunning to look good.
        -->
       <attr name="simple" boolvalue="false" />
       <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/vcscore/resources/templatesVCSFS.html"/>
       <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcscore.wizard.Bundle" />
       </folder>
     </folder>
   </folder>

  <folder name="Windows2">
      <folder name="Modes">
          <folder name="output">
              <file name="VCSCommandOutput.wstcref" url="CommandOutput.wstcref" />
          </folder>
      </folder>
      <folder name="Components">
          <file name="VCSCommandOutput.settings" url="CommandOutputTopComponent.settings" />
      </folder>
  </folder>

</filesystem>
