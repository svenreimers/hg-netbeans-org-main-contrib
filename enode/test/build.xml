<project name="openidex/enode/test" basedir="." default="all" >
    <!-- build tested modules  -->
    <property name="testmodules" location="${basedir}/../testmodule"/>

    <!-- Name of tested module -->
    <property name="xtest.module" value="openidex/enode"/>
    
    <import file="../../../nbbuild/templates/xtest.xml"/>
    
    <!-- default testtypes, attributes used when no value is supplied from command line -->
    <property name="xtest.testtype" value="unit"/>
    <property name="xtest.attribs" value="stable,ide"/>
    
    <target name="-buildtests">
        <ant antfile="${testmodules}/build.xml" dir="${testmodules}" target="netbeans"/>
    </target>
    
    <target name="buildtests" depends="-buildtests,xtest.buildtests">
    </target>
    
    <target name="-cleantests">
        <ant antfile="${testmodules}/build.xml" dir="${testmodules}" target="clean"/>
    </target>
    
    <target name="cleantests" depends="-cleantests,xtest.cleantests">
    </target>
    
</project>
