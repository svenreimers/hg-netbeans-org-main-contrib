<?xml version="1.0"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.0//EN"
                             "http://www.netbeans.org/dtds/EditorFontsColors-1_0.dtd">
<fontscolors>
    <fontcolor foreColor="#FFFFFF" syntaxName="dtd-symbol">
        <font name="Monospaced" size="11" style="bold"/>
    </fontcolor>
    <fontcolor bgColor="#666666" syntaxName="guarded"/>
    <fontcolor foreColor="#CCFFFF" syntaxName="dtd-plain"/>
    <fontcolor syntaxName="highlight-caret-row"/>
    <fontcolor bgColor="#000000" foreColor="#FFFFFF" syntaxName="default">
        <font name="Monospaced" size="11" style="plain"/>
    </fontcolor>
    <elementcolor color="#FFFFFF" name="caret-color-insert-mode"/>
    <elementcolor color="#FFFFFF" name="caret-color-overwrite-mode"/>
</fontscolors>
