<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<?xml-stylesheet type="text/xml" href="http://openide.netbeans.org/fs/filesystem.xsl"?>

<filesystem>

    <folder name="Services">
        <folder name="Hidden">

            <file name="LaTeXGUIProjectFactory.instance">
                <attr name="instanceClass" stringvalue="org.netbeans.modules.latex.guiproject.LaTeXGUIProjectFactory"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.project.ProjectFactory"/>
            </file>
            
        </folder>
    </folder>
    
    <folder name="Actions">
        <folder name="Project">
            <file name="Show.instance">
                <attr methodvalue="org.netbeans.modules.latex.guiproject.ActionsFactory.createMainProjectShowAction" name="instanceCreate"/>
            </file>
        </folder>
    </folder>
    
    <folder name="Menu">
      <folder name="RunProject">
            <file name="Show.shadow">
	        <attr name="originalFile" stringvalue="Actions/Project/Show.instance" />
            </file>
        </folder>
    </folder>
    
    <folder name="Templates">
        <folder name="Project">
            <!--Put the LaTeX project behind Web and Standard project
                (the projects should be sorted by importance, and LaTeX
                 is less important than these two):
              -->
            <attr name="Web/LaTeX" boolvalue="true" />
            <attr name="Standard/LaTeX" boolvalue="true" />
            
            <folder name="LaTeX">
                <file name="new-latex-project.xml">
                    <attr name="template" boolvalue="true" />
                    <attr name="instantiatingIterator" newvalue="org.netbeans.modules.latex.guiproject.wizard.NewLaTeXGUIProjectWizardIterator" />
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.latex.guiproject.Bundle" />
                </file>
            </folder>
        </folder>
    </folder>
    
    <folder name="latex">
        <folder name="guiproject">
            <file name="build-settings.properties" url="build-settings.properties">
            </file>
            <file name="document.tex" url="document.tex">
            </file>
        </folder>
    </folder>

    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="org-netbeans-modules-latex-compiler-error.xml" url="compiler-error-annotation.xml" />
        </folder>
    </folder>
</filesystem>
