/* Fixed length struct */
struct S {
  long a, b;
  char c;
};

/* Variable length struct */
struct S2 {
  string a;
  S      x;
};


interface foo {
  S bar( in S s1, out S ss2, inout S s3 );
  S2 bar1( in S2 s1, out S2 ss2, inout S2 s3 );
};

