/* Recursive struct */
struct S {
  string name;
  sequence<S,1> left;
  sequence<S,1> right;
};

/* Nested recursive struct */
struct S2 {
  struct S3 {
    sequence<S2> c;
  } s;
};

/* Nested recursive union */
union UA switch (boolean) {
 case TRUE:
   struct t_st {
     sequence<UA> a;
   } st;
 case FALSE:
   short b;
};

struct S4 {
  struct S5 {
    struct S6 {
      sequence<S4> s14;
      sequence<S5> s15;
      sequence<S6> s16;
    } s26;
  } s25;
};

interface foo {
  void bar( in S sZX );
};

