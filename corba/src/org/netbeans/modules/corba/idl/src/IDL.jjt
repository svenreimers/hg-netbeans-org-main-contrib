*/
String string_type() :
{}
{
  {
    String name, tmp;
  }
  "string" { name = "string";} 
  [ "<" {name = name + "<";} tmp = positive_int_const() { name += tmp;} ">" {name = name + ">";} ]
  { return name;}
}

