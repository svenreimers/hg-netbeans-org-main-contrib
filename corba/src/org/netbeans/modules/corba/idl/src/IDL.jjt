Vector inheritance_spec() :
{}
{
  {
    Vector inherited_from = new Vector ();
    String name = "";
  }
  ":" name = scoped_name() { inherited_from.addElement (name);} 
  ( "," name = scoped_name() { inherited_from.addElement (name);} )*
  {
    return inherited_from;
  }
}

