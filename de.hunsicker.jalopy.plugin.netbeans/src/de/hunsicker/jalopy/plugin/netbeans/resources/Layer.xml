<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN"
          "http://www.netbeans.org/dtds/filesystem-1_1.dtd">

<filesystem>
  <folder name="Menu">
    <folder name="BuildProject">
      <file name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance"/>
      <attr name="org-netbeans-modules-project-ui-CompileSingle.instance/de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance" boolvalue="true" />
	  <attr name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance/Separator2.instance" boolvalue="true"/>
    </folder>

    <folder name="Tools">
      <file name="de-hunsicker-jalopy-plugin-netbeans-SettingsAction.instance"/>
      <attr name="org-netbeans-core-actions-OptionsAction.instance/de-hunsicker-jalopy-plugin-netbeans-SettingsAction.instance" boolvalue="true" />
    </folder>
  </folder>

  <folder name="System">
    <file name="de-hunsicker-jalopy-plugin-netbeans-FormatAction.instance"/>
    <file name="de-hunsicker-jalopy-plugin-netbeans-SettingsAction.instance"/>
  </folder>

  <folder name="Shortcuts">
	<file name="CS-F10.instance">
		<attr name="instanceClass" stringvalue="de.hunsicker.jalopy.plugin.netbeans.FormatAction"/>
	</file>
  </folder>

  <folder name="Editors">
    <folder name="AnnotationTypes">
      <file name="de-hunsicker-jalopy-plugin-netbeans-error.xml" url="error-annotation.xml"/>
      <file name="de-hunsicker-jalopy-plugin-netbeans-warn.xml" url="warn-annotation.xml"/>
    </folder>
  </folder>
  
</filesystem>