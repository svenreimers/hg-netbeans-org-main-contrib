<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <goals>
            <goal>package</goal>
            <goal>ios:deploy</goal>
        </goals>
        <properties>
            <skipTests>true</skipTests>
        </properties>
    </action>
    <action>
        <actionName>debug</actionName>
        <goals>
            <goal>package</goal>
            <goal>ios:deploy</goal>
        </goals>
        <properties>
            <skipTests>true</skipTests>
            <jpda.listen>true</jpda.listen>
            <jpdaAddress>${jpda.address}</jpdaAddress>
            <runMode>Debug</runMode>
        </properties>
    </action>
</actions>
