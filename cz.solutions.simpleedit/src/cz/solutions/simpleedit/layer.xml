<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Build">
            <file name="cz-solutions-simpleedit-KeepEditorAction.instance"/>
        </folder>
        <folder name="Edit">
            <file name="cz-solutions-simpleedit-PreviousFileAction.instance"/>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="cz-solutions-simpleedit-ClosingTabsAdvancedOption.instance"/>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="Edit">
            <file name="cz-solutions-simpleedit-PreviousFileAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/cz-solutions-simpleedit-PreviousFileAction.instance"/>
            </file>
            <attr name="cz-solutions-simpleedit-PreviousFileAction.shadow/cz-solutions-simpleedit-ToggleMainModeAction.shadow" boolvalue="true"/>
            <file name="cz-solutions-simpleedit-ToggleMainModeAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Build/cz-solutions-simpleedit-KeepEditorAction.instance"/>
            </file>
            <attr name="cz-solutions-simpleedit-ToggleMainModeAction.shadow/org-openide-actions-CutAction.instance" boolvalue="true"/>
        </folder>
    </folder>
</filesystem>
