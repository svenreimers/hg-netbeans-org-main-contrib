<project name="vcsgeneric" default="netbeans" basedir=".">
    <import file="../nbbuild/templates/projectized.xml"/>

    <property name="stdprofiles" value="cvsprofiles, pvcs, vss"/>

    <target name="profile-cvsprofiles">
        <echo message="Building CVS profiles..."/>
        <ant dir="profiles/cvsprofiles" target="netbeans"/>
    </target>
    <target name="profile-pvcs">
        <echo message="Building PVCS profiles..."/>
        <ant dir="profiles/pvcs" target="netbeans"/>
    </target>
    <target name="profile-vss">
        <echo message="Building VSS profiles..."/>
        <ant dir="profiles/vss" target="netbeans"/>
    </target>

</project>
