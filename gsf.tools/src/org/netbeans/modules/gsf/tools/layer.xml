<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Ruby">
            <file name="org-netbeans-modules-gsf-browser-DumpIndex.instance"/>
            <file name="org-netbeans-modules-gsf-tools-FormatDir.instance"/>
        </folder>
        <folder name="Window">
            <file name="org-netbeans-modules-gsf-browser-AstViewerAction.instance"/>
            <file name="org-netbeans-modules-gsf-tools-lexing-TokenSpyAction.instance"/>
            <file name="org-netbeans-modules-gsf-tools-lucene-IndexBrowserAction.instance"/>
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Window">
            <folder name="Ruby Development">
                <file name="org-netbeans-modules-gsf-browser-DumpIndex.shadow">
                    <attr name="originalFile" stringvalue="Actions/Ruby/org-netbeans-modules-gsf-browser-DumpIndex.instance"/>
                </file>
                <attr name="org-netbeans-modules-gsf-browser-DumpIndex.shadow/AstViewerAction.shadow" boolvalue="true"/>
                <file name="AstViewerAction.shadow">
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-gsf-browser-AstViewerAction.instance"/>
                </file>
                <file name="org-netbeans-modules-gsf-tools-FormatDir.shadow">
                    <attr name="originalFile" stringvalue="Actions/Ruby/org-netbeans-modules-gsf-tools-FormatDir.instance"/>
                </file>
                <attr name="org-netbeans-modules-gsf-tools-FormatDir.shadow/IndexBrowserAction.shadow" boolvalue="true"/>
                <file name="IndexBrowserAction.shadow">
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-gsf-tools-lucene-IndexBrowserAction.instance"/>
                </file>
                <file name="TokenSpyAction.shadow">
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-gsf-tools-lexing-TokenSpyAction.instance"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="Windows2">
        <folder name="Components">
            <file name="AstViewer.settings" url="AstViewerSettings.xml"/>
            <file name="IndexBrowserTopComponent.settings" url="IndexBrowserTopComponentSettings.xml"/>
            <file name="TokenSpyTopComponent.settings" url="TokenSpyTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="IndexBrowserTopComponent.wstcref" url="IndexBrowserTopComponentWstcref.xml"/>
            </folder>
            <folder name="explorer">
                <file name="AstViewer.wstcref" url="AstViewerWstcref.xml"/>
            </folder>
            <folder name="output">
                <file name="TokenSpyTopComponent.wstcref" url="TokenSpyTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
