<!DOCTYPE filesystem PUBLIC '-//NetBeans//DTD Filesystem 1.1//EN' 'http://www.netbeans.org/dtds/filesystem-1_1.dtd'>

<filesystem>
    <folder name="xml">
        <folder name="memory">
            <folder name="org">
                <folder name="netbeans">
                    <folder name="modules">
                        <folder name="tasklist">
                            <folder name="filter">
                                <file name="FilterRepository">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_Filters_1_0.instance"/>
                                 </file>

                                <file name="BooleanFilterCondition">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_BooleanCondition_1_0.instance"/>
                                </file>
                                <file name="IntegerFilterCondition">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_IntegerCondition_1_0.instance"/>
                                </file>
                                <file name="StringFilterCondition">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_StringCondition_1_0.instance"/>
                                </file>
                                <file name="DateFilterCondition">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_DateCondition_1_0.instance"/>
                                </file>
                                <file name="PriorityCondition">
                                    <attr name="settings.providerPath"
                                        stringvalue="xml/lookups/NetBeans_org_netbeans_modules_tasklist/DTD_PriorityCondition_1_0.instance"/>
                                </file>
                            </folder>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="entities">
            <!--Entity registration-->
            <folder name="NetBeans_org_netbeans_modules_tasklist">
                <!-- filter repository -->
                <file name="DTD_Filters_1_0" url="filters-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD Filters 1.0//EN"/>
                </file>

	        <!-- condtitions -->
                <file name="DTD_IntegerCondition_1_0" url="conditions-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD IntegerCondition 1.0//EN"/>
                </file>
                <file name="DTD_StringCondition_1_0" url="conditions-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD StringCondition 1.0//EN"/>
                </file>
                <file name="DTD_DateCondition_1_0" url="conditions-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD DateCondition 1.0//EN"/>
                </file>
                <file name="DTD_PriorityCondition_1_0" url="conditions-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD PriorityCondition 1.0//EN"/>
                </file>
                <file name="DTD_BooleanCondition_1_0" url="conditions-1_0.dtd">
                    <attr name="hint.originalPublicID"
                        stringvalue="-//NetBeans org.netbeans.modules.tasklist//DTD BooleanCondition 1.0//EN"/>
                </file>


            </folder>
        </folder>
        <folder name="lookups">
            <folder name="NetBeans_org_netbeans_modules_tasklist">

	<!-- filter repository  -->
                <file name="DTD_Filters_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.filter.FilterRepositoryConvertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.filter.FilterRepository"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.filter.FilterRepository"/>
                </file>

	<!-- integer condition -->
                <file name="DTD_IntegerCondition_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.filter.IntegerFilterCondition$Convertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.filter.IntegerFilterCondition"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.filter.IntegerFilterCondition"/>
                </file>

	<!-- string condition -->
                <file name="DTD_StringCondition_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.filter.StringFilterCondition$Convertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.filter.StringFilterCondition"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.filter.StringFilterCondition"/>
                </file>

	<!-- date condition -->
                <file name="DTD_DateCondition_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.filter.DateFilterCondition$Convertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.filter.DateFilterCondition"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.filter.DateFilterCondition"/>
                </file>

	<!-- boolean condition -->
                <file name="DTD_BooleanCondition_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.filter.BooleanFilterCondition$Convertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.filter.BooleanFilterCondition"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.filter.BooleanFilterCondition"/>
                </file>

	<!-- priority condition -->
                <file name="DTD_PriorityCondition_1_0.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.api.settings.Factory.create"/>
                    <attr name="settings.convertor" methodvalue="org.netbeans.modules.tasklist.suggestions.PriorityCondition$Convertor.create"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.modules.tasklist.suggestions.PriorityCondition"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.modules.tasklist.suggestions.PriorityCondition"/>
                </file>

            </folder>
        </folder>
    </folder>

</filesystem>

