<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE filesystem PUBLIC '-//NetBeans//DTD Filesystem 1.1//EN' 'http://www.netbeans.org/dtds/filesystem-1_1.dtd'>

<filesystem>
    <folder name="Actions" >
        <folder name="Window">
            <file name="org-netbeans-modules-targets-ShowTargets.instance"/>
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Window">
            <file name="ShowTargets.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-targets-ShowTargets.instance"/>
            </file>
        </folder>
    </folder>

    <folder name="Toolbars">
        <folder name="Targets">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.targets.Bundle" />
            <file name="org-netbeans-modules-targets-PresenterSourceAction.instance"/>
        </folder>
    </folder>    

    <!-- Window system configuration --> 
    <folder name="Windows2">
        <folder name="Modes">
            <file name="targets.wsmode" url="modes/targets.wsmode" />
            <folder name="targets">
                <file name="targets.wstcref" url="modes/targets/targets.wstcref" />
            </folder>
        </folder>
        <folder name="Components">
            <file name="targets.settings" url="components/targets.settings" />
        </folder>
    </folder>   
    
    <folder name="Targets">
        <!-- Persisted targets will go here -->
    </folder>
   
</filesystem>

