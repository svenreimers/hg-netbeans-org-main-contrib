<project name="zeroadmin tests build script" basedir="." default="all">
    <!-- Name of tested module -->
    <property name="xtest.module" value="contrib/zeroadmin"/>
    
    <import file="../../../nbbuild/templates/xtest.xml"/>
    
    <!-- default testtypes, attributes used when no value is supplied from command line -->
    <property name="xtest.testtype" value="unit"/>
    <property name="xtest.attribs" value="stable"/>
    
    <target name="init" depends="openide/fs-test,default.init">
    </target>
    
    <target name="openide/fs-test" depends="check-openide/fs-test" unless="openide/fs-test.built">
        <ant dir="../../../openide/fs/test" target="buildtests"/>
    </target>
    <target name="check-openide/fs-test">
        <available property="openide/fs-test.built" file="../../../openide/fs/build/test/unit/classes"/>
    </target>
</project>

