<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook-slides+xml">
            <xml-rule>
                <doctype public-id="-//Norman Walsh//DTD Slides XML V3.2.0//EN"/>
                <element name="slides"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook+xml">
            <xml-rule>
           <doctype public-id="-//OASIS//DTD DocBook XML V4.2//EN"/>
                <element name="article"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook+xml">
            <xml-rule>
           <doctype public-id="-//OASIS//DTD DocBook XML V4.2//EN"/>
                <element name="book"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook+xml">
            <xml-rule>
                <element name="chapter"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook+xml">
            <xml-rule>
                <element name="appendix"/>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-docbook+xml">
            <xml-rule>
                <element name="section"/>
            </xml-rule>
        </resolver>
    </file>
</MIME-resolver>
