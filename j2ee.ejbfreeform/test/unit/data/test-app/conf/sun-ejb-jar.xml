<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 EJB 2.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_2_1-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>RunSessionBean</ejb-name>
      <jndi-name>ejb/RunSessionBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>WSEndpoint</ejb-name>
      <webservice-endpoint>
        <port-component-name>WSEndpoint</port-component-name>
        <endpoint-address-uri>webservice/WSEndpoint</endpoint-address-uri>
      </webservice-endpoint>
    </ejb>
    <cmp-resource>
      <jndi-name>jdbc/derby_net</jndi-name>
    </cmp-resource>
  </enterprise-beans>
</sun-ejb-jar>
