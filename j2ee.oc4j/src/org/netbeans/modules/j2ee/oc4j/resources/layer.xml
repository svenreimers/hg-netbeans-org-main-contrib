<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="J2EE">
        <folder name="DeploymentPlugins">
            <folder name="OC4J">    
                <file name="Descriptor" url="deployment.xml"/>
                
                <file name="OC4JDeploymentFactory.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.j2ee.oc4j.OC4JDeploymentFactory.getDefault"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.oc4j.OC4JDeploymentFactory"/>
                    <attr name="instanceOf" stringvalue="javax.enterprise.deploy.spi.factories.DeploymentFactory"/>
                </file>
                
                <file name="OC4JRegistryNodeFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.oc4j.nodes.OC4JRegistryNodeFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.oc4j.nodes.OC4JRegistryNodeFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.spi.RegistryNodeFactory"/>
                </file>
                
                <file name="OC4JJ2eePlatformFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.oc4j.ide.OC4JJ2eePlatformFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.oc4j.ide.OC4JJ2eePlatformFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.spi.J2eePlatformFactory"/>
                </file>
                
                <file name="OC4JOptionalDeploymentManagerFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.oc4j.OC4JOptionalDeploymentManagerFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.oc4j.OC4JOptionalDeploymentManagerFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.spi.OptionalDeploymentManagerFactory"/>
                </file>
                
                <file name="OC4JModuleConfigurationFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.j2ee.oc4j.config.OC4JModuleConfigurationFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.j2ee.oc4j.config.OC4JModuleConfigurationFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.spi.config.ModuleConfigurationFactory"/>
                </file>
                
                <folder name="DeploymentFileNames">
                    <folder name="WAR">
                        <file name="WEB-INF\orion-web.xml"/>
                    </folder>
                    
                    <folder name="EJB">
                        <file name="META-INF\orion-ejb-jar.xml"/>
                    </folder>
                    
                    <folder name="EAR">
                        <file name="META-INF\orion-application.xml"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>