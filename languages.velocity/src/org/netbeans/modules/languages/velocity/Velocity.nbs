########### tokens #############################################################

TOKEN:vm_comment:( "#*" - "*#" )
TOKEN:vm_comment:( "##" [^ "\n" "\r"]+ )
TOKEN:vm_keyword:(
    "#set" |
    "#foreach" |
    "#if" |
    "#elseif" |
    "#parse" |
    "#macro" |
    "#include"
):<IN_VM>
TOKEN:vm_keyword:( 
    "#end" |
    "#else" |
    "#stop"
)
TOKEN:vm_identifier:( 
    "$" "!"? "{"?
    ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* "}"?
)
TOKEN:vm_identifier:( 
    "$" "{"?
    ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* 
    "." ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* "}"?
)
TOKEN:vm_identifier:( 
    "$"  "{"?
    ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* 
    "." ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* 
    "("
):<IN_PARAMS>
<IN_PARAMS> {
    TOKEN:vm_string: (
        "\"" ( 
            [^ "\\" "\"" "\n" "\r"] |
            ("\\" (
                . |
                (["0"-"7"] ["0"-"7"] ["0"-"7"]) |
                ("x" ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"]) |
                ("u" ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"])
            ))
        )* 
        "\""
    )
    TOKEN:vm_number: (
        ["0"-"9"] ['l' 'L']? |
        ["1"-"9"] ["0"-"9"]* ['l' 'L']? |
        "0" ["0"-"7"]+ ['l' 'L']? |
        "0" ["x" "X"] ["0"-"9" "a"-"f" "A"-"F"]+ ['l' 'L']? |
        ["0"-"9"]+ "." ["0"-"9"]* (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]? |
        "." ["0"-"9"]+ (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]? |
        ["0"-"9"]+ ["e" "E"] ["+" "-"]? ["0"-"9"]+ ["f" "F" "d" "D"]? |
        ["0"-"9"]+ (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]
    )
    TOKEN:vm_identifier:( "$"? ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* )
    TOKEN:vm_operator:( "," )
    TOKEN:vm_operator:( ")" "}"? ):<DEFAULT>
}
<IN_VM> {
    TOKEN:vm_whitespace:( [" " "\t" "\n" "\r"]+ )
    TOKEN:vm_operator {
        pattern:( "(" );
        end_state:"IN_VM2";
        counter:"i++";
    }
}
<IN_VM2> {
    TOKEN:vm_identifier:( "$"? ["a"-"z" "A"-"Z"] ["a"-"z" "A"-"Z" "0"-"9" "-" "_"]* )
    TOKEN:vm_operator:( "." | "(" | ")" | "[" | "]" | "=" | "," )
    TOKEN:vm_operator {
        pattern:( "(" );
        end_state:"IN_VM2";
        counter:"i++";
    }
    TOKEN:vm_operator {
        pattern:( ")" );
        end_state:"IN_VM2";
        end_state2:"DEFAULT";
        counter:"i--";
    }
    TOKEN:vm_whitespace:( " " | "\t" )
    TOKEN:vm_eol:( ["\n" "\r"]+ ):<DEFAULT>
    TOKEN:vm_string: (
        "\"" ( 
            [^ "\\" "\"" "\n" "\r"] |
            ("\\" (
                . |
                (["0"-"7"] ["0"-"7"] ["0"-"7"]) |
                ("x" ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"]) |
                ("u" ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"] ["0"-"9" "a"-"f" "A"-"F"])
            ))
        )* 
        "\""
    )
    TOKEN:vm_number: (
        ["0"-"9"] ['l' 'L']? |
        ["1"-"9"] ["0"-"9"]* ['l' 'L']? |
        "0" ["0"-"7"]+ ['l' 'L']? |
        "0" ["x" "X"] ["0"-"9" "a"-"f" "A"-"F"]+ ['l' 'L']? |
        ["0"-"9"]+ "." ["0"-"9"]* (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]? |
        "." ["0"-"9"]+ (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]? |
        ["0"-"9"]+ ["e" "E"] ["+" "-"]? ["0"-"9"]+ ["f" "F" "d" "D"]? |
        ["0"-"9"]+ (["e" "E"] ["+" "-"]? ["0"-"9"]+)? ["f" "F" "d" "D"]
    )
    TOKEN:vm_comment:( "##" [^ " " "\t" "\n" "\r"]+ ):<DEFAULT>
}

TOKEN:text:( [^ "#" "$"]+ )
TOKEN:text:( "#" )
TOKEN:text:( "$" )

COLOR:vm_comment: {
    background_color:"#e6ffe6";
}
COLOR:vm_keyword: {
    background_color:"#e6ffe6";
}
COLOR:vm_number: {
    background_color:"#e6ffe6";
}
COLOR:vm_string: {
    background_color:"#e6ffe6";
}
COLOR:vm_whitespace: {
    background_color:"#e6ffe6";
}
COLOR:vm_identifier: {
    background_color:"#e6ffe6";
}
COLOR:vm_operator: {
    background_color:"#e6ffe6";
}

