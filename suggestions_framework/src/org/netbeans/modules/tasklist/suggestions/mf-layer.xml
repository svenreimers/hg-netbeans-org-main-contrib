<!DOCTYPE filesystem PUBLIC '-//NetBeans//DTD Filesystem 1.1//EN' 'http://www.netbeans.org/dtds/filesystem-1_1.dtd'>

<filesystem>
    <!-- default order for tasklist provided suggestions:
      Tasks Due
      Compilation Errors
      Rule Violations
      Source Tasks
      Javadoc Problems
      Copyright Problems
     -->
    <folder name="Suggestions">
      <folder name="Types">
        <attr name="org-netbeans-modules-tasklist-usertasks-duetasks.xml/org-netbeans-modules-tasklist-javaparser-parse-error.xml" boolvalue="true" />
        <attr name="org-netbeans-modules-tasklist-javaparser-parse-error.xml/pmd-rule_violation.xml" boolvalue="true" />
        <attr name="pmd-rule_violation.xml/org-netbeans-modules-tasklist-docscan-scanned_task.xml" boolvalue="true" />
        <attr name="org-netbeans-modules-tasklist-docscan-scanned_task.xml/org-netbeans-modules-tasklist-javadoc-docerror.xml" boolvalue="true" />
        <attr name="org-netbeans-modules-tasklist-javadoc-docerror.xml/org-netbeans-modules-tasklist-docscan-copyright_suggestion.xml" boolvalue="true" />
      </folder>
    </folder>

    <folder name="Editors"> 
        <folder name="AnnotationTypes"> 
            <file name="SuggestionAnno.xml" url="SuggestionAnno.xml"/>
        </folder>
    </folder>

    <folder name="Actions">
	<folder name="View">
            <file name="org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance" />
        </folder>
    </folder>        
            
    <folder name="Menu">
        <folder name="View">
            <file name="org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance" />
            <attr name="org-netbeans-modules-tasklist-usertasks-ViewTasksAction.instance/org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance" boolvalue="true" />
            <attr name="org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance/org-netbeans-modules-scripting-ConsoleAction.instance" boolvalue="true" />
         </folder>
     </folder>

     <folder name="Toolbars">  
         <folder name="Task">
             <file name="org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance" />
             <attr name="org-netbeans-modules-tasklist-usertasks-ViewTasksAction.instance/org-netbeans-modules-tasklist-suggestions-SuggestionsAction.instance" boolvalue="true" />
        </folder>
    </folder>

    <folder name="xml">
        <folder name="entities">
            <folder name="NetBeans" >
                <file name="DTD_suggestion_type_1_0" url="nbres:/org/netbeans/modules/tasklist/suggestions/suggestion-type-1_0.dtd"/>
            </folder>    
        </folder>
        <folder name="lookups">
            <folder name="NetBeans" >
               <file name="DTD_suggestion_type_1_0.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tasklist.suggestions.SuggestionTypeProcessor" />
               </file>
            </folder>    
        </folder>
    </folder>

</filesystem>

