<?xml version="1.0"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>
    <folder name="Mount">
        <folder name="Javadoc">
            <file name="org-netbeans-modules-testtools-jemmy-javadoc.xml">
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.jemmysupport.Bundle" />
	      <![CDATA[
		<!DOCTYPE Javadoc PUBLIC "-//NetBeans IDE//DTD JavadocLibrary//EN" "http://www.netbeans.org/dtds/JavadocLibrary-1_0.dtd">
		<Javadoc>
		    <Archive name="docs/jemmy-javadoc.zip"/>
		</Javadoc>
	      ]]>
            </file>
        </folder>
        <folder name="java">
            <file name="org-netbeans-modules-testtools-jemmy-library.xml">
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.jemmysupport.Bundle" />
	      <![CDATA[
	          <!DOCTYPE Library PUBLIC "-//NetBeans IDE//DTD JavaLibrary//EN" "http://www.netbeans.org/dtds/JavaLibrary-1_0.dtd">
	          <Library>
	              <Archive name="modules/ext/jemmy.jar"/>
	          </Library>
	      ]]>
           </file>
      </folder>
    </folder>
    <folder name="Menu">
        <folder name="Tools">
                <file name="org-netbeans-modules-jemmysupport-generator-GeneratorAction.instance"/>
                <file name="org-netbeans-modules-jemmysupport-bundlelookup-BundleLookupAction.instance"/>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="Jemmy Support">
                <file name="org-netbeans-modules-jemmysupport-generator-GeneratorAction.instance"/>
                <file name="org-netbeans-modules-jemmysupport-bundlelookup-BundleLookupAction.instance"/>
        </folder>
    </folder>
</filesystem>
