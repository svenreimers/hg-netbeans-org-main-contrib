            <file name="Smarty">
                <attr name="include" stringvalue="config/Preferences/org/netbeans/modules/php/smarty/smarty.properties"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.php.smarty.resources.Bundle#LBL_SmartyExportName"/>
                <attr name="position" intvalue="500"/>
            </file>
        </folder>
    </folder>

    <!-- PHP registrations -->
    <folder name="PHP">
        <folder name="Frameworks">
            <file name="org-netbeans-modules-php-smarty-SmartyPhpFrameworkProvider.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.smarty.SmartyPhpFrameworkProvider.getInstance"/>
                <attr name="position" intvalue="300"/>
            </file>
        </folder>
    </folder>

</filesystem>
