<!--
    Description: export format for user tasks
-->

<!ELEMENT tasks (task)*>

<!--
    due, created, completed, start and modified - date format: YYYY-MM-DDThh:mm:ssTZD
        (see http://www.w3.org/TR/NOTE-datetime)
    line - 1 = the first line in a file
    effort - effort in minutes
    spent - spent time in minutes
    
    progress-computed, effort-computed and spent-time-computed will be ignored
    and only here for backward compatibility (see the new attribute
    values-computed)
-->
<!ELEMENT task (summary, details?, work-periods?, (task)*)>
<!ATTLIST task
    priority (high|medium-high|medium|medium-low|low)  "medium"
    category CDATA #IMPLIED
    progress CDATA #REQUIRED
    progress-computed (yes|no) "no"
    effort CDATA #REQUIRED
    effort-computed (yes|no) "no"
    due CDATA #IMPLIED
    file CDATA #IMPLIED
    line CDATA #IMPLIED
    created CDATA #REQUIRED
    modified CDATA #REQUIRED
    completed CDATA #IMPLIED
    spent-time CDATA #REQUIRED
    spent-time-computed (yes|no) "no"
    owner CDATA #IMPLIED
    start CDATA #IMPLIED
    values-computed (yes|no) "no"
>

<!ELEMENT details (#PCDATA)>
<!ELEMENT work-periods (period)+>

<!ELEMENT period EMPTY>

<!--
    duration - duration of a work period in minutes
-->
<!ATTLIST period
    start CDATA #REQUIRED
    duration CDATA #REQUIRED
>
