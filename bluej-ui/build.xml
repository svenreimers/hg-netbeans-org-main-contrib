<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="netbeans" name="ide/bluejbranding">
    <description>Builds, tests, and runs the project org.netbeans.bluej.branding</description>
    <import file="../../nbbuild/templates/projectized.xml"/>
    <target name="branding" depends="init" if="branding.token">
        <taskdef name="branding" classname="org.netbeans.nbbuild.Branding" classpath="../../nbbuild/nbantext.jar"/>
        <property name="branding.dir" location="branding"/>
        <mkdir dir="${cluster}"/>
        <branding cluster="${cluster}" overrides="${branding.dir}" token="${branding.token}"/>
    </target>
    
</project>
