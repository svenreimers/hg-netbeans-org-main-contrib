<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Loaders">
        <folder name="text">
            <folder name="x-mashup+xml">
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="MashupResolver.xml" url="MashupResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.edm.editor.Bundle"/>
            </file>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="SOA">
            <file name="untitled.edm" url="MashupTemplate.xml">
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/edm/editor/resources/mashup.png"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.edm.editor.Bundle"/>
                <attr name="instantiatingIterator" newvalue="org.netbeans.modules.edm.editor.wizard.MashupWizardIterator"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/edm/editor/wizard/mashup.html"/>
                <attr name="templateCategory" stringvalue="XML"/>
            </file>
        </folder>
    </folder>    
</filesystem>
