<project name="enode-test" default="netbeans" basedir=".">

    <import file="../../../nbbuild/templates/projectized.xml"/>
    <target name="compile" depends="init">
        <mkdir dir="${build.classes.dir}"/>
        <javac srcdir="${src.dir}" destdir="${build.classes.dir}" debug="${build.compiler.debug}" deprecation="${build.compiler.deprecation}" optimize="${build.compiler.optimize}" source="${javac.source}" includeantruntime="false">
            <classpath refid="cp"/>
            <include name="org/netbeans/modules/enode/test/*.java"/>
        </javac>
        <copy todir="${build.classes.dir}">
          <fileset dir="${src.dir}" excludesfile="${nb_all}/nbbuild/standard-jar-excludes.txt"/>
        </copy>
    </target>
 

</project>
