<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd" [
  <!ENTITY includefiles_commands SYSTEM "layer-includes-commands.xml">
  <!ENTITY includefiles_config SYSTEM "layer-includes-config.xml">
  <!ENTITY includefiles_configRB SYSTEM "layer-includes-configRB.xml">
  <!ENTITY includefiles_list SYSTEM "layer-includes-list.xml">
]>
<filesystem>
  <folder name="org">
    <folder name="netbeans">
      <folder name="modules">
        <folder name="vcs">
          <folder name="profiles">
            <folder name="vss">
              <folder name="commands">
                &includefiles_commands;
              </folder>
              <folder name="config">
                &includefiles_configRB;
              </folder>
              <folder name="list">
                &includefiles_list;
              </folder>
            </folder>
          </folder>
        </folder>
      </folder>
    </folder>
  </folder>
  <folder name="vcs">
    <folder name="config">
      &includefiles_config;
    </folder>
    <folder name="versioningMenu">
      <file name="vss.xml" url="vss.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.vcs.profiles.vss.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:org/openide/resources/actions/empty.gif" />          
            <attr name="os.name" stringvalue="win" />
      </file>
    </folder>
  </folder>
</filesystem>
