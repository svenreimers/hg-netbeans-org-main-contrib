<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-netbeans-modules-declarationandjavadoc-DeclarationAction.instance"/>
            <file name="org-netbeans-modules-declarationandjavadoc-JavadocAction.instance"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <attr name="InspectorAction.shadow/DeclarationAction.shadow" boolvalue="true"/>
            <file name="DeclarationAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-declarationandjavadoc-DeclarationAction.instance"/>
            </file>
            <attr name="DeclarationAction.shadow/JavadocAction.shadow" boolvalue="true"/>
            <file name="JavadocAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-declarationandjavadoc-JavadocAction.instance"/>
            </file>
            <attr name="JavadocAction.shadow/SwitchToRecentDocumentAction.shadow" boolvalue="true"/>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="DeclarationTopComponent.settings" url="DeclarationTopComponentSettings.xml"/>
            <file name="JavadocTopComponent.settings" url="JavadocTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="output">
                <file name="DeclarationTopComponent.wstcref" url="DeclarationTopComponentWstcref.xml"/>
                <file name="JavadocTopComponent.wstcref" url="JavadocTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
