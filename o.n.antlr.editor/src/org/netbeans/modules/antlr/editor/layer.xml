<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="antlr">
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="org-netbeans-antlr-editor-coloring.xml" url="fontsColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.antlr.editor.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>    
    </folder>
    
    <folder name="Loaders">
        <folder name="text">
            <folder name="antlr">
                
                <!-- GSF -->
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/antlr/editor/build.png"/>
                <attr name="iconBase" stringvalue="org/netbeans/modules/antlr/editor/build.png"/>
            </folder>
        </folder>
    </folder>
    
</filesystem>
