<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <!-- Perspective Actions -->
    <folder name="Actions">
        <folder name="Perspective">
            <!-- Qucik Action -->
            <file name="org-netbeans-modules-perspective-actions-QuickSwitch.instance"/>
            <!-- Reset Action -->
            <file name="org-netbeans-modules-perspective-menus-ResetAction.instance"/>
            <!-- Save as Action -->
            <file name="org-netbeans-modules-perspective-menus-SaveAsAction.instance"/>
        </folder>
    </folder>
    
    <!-- Perspective Menu -->
    <folder name="Menu">
        <folder name="Perspective">
            <!-- i18n attribute -->
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.perspective.Bundle"/>
            <!-- position attribute (between tools and window menu)-->
            <attr name="position" intvalue="1150"/>
            <!-- Save as menu -->
            <file name="org-netbeans-perspective-menus-SaveAsAction.shadow">
                <attr name="position" intvalue="100"/>
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-SaveAsAction.instance"/>
            </file>
            <!-- Reset menu -->
            <file name="org-netbeans-modules-perspective-menus-ResetAction.shadow">
                <attr name="position" intvalue="200"/>
                <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-menus-ResetAction.instance"/>
            </file>
            
        </folder>
    </folder>
    
    <!-- Options Dialog -->
    <folder name="OptionsDialog">
        <!-- Perspctive Caregory -->
        <file name="org-netbeans-modules-perspective-options-PerspectiveOptionsCategory.instance"/>
        <attr name="position" intvalue="800"/>
    </folder>
    <!-- Shortcuts -->
    <folder name="Shortcuts">
        <!--Quick Switch Shortcut -->
        <file name="OS-P.shadow">
            <attr name="originalFile" stringvalue="Actions/Perspective/org-netbeans-modules-perspective-actions-QuickSwitch.instance"/>
        </file>
    </folder>
    
    <!--  Perspective folder-->
    <folder name="perspectives">
        <!--Config folder contains  serialized perspectives-->
        <folder name="config">
            
        </folder>
        <!-- built-in Perspectives Registry -->
        <folder name="builtin">
            <!--default java Perspective-->
            <folder name="default">
                <file name="java">
                    <attr name="class" stringvalue="org.netbeans.modules.perspective.builtin.BuiltInDefaultView" />            
                </file>  
                <!-- java browser Perspective-->
                <file name="java_browsing">
                    <attr name="class" stringvalue="org.netbeans.modules.perspective.builtin.BuiltInJavaBrowsingView" />         
                </file>
                <!--debug java Perspective-->
                <file name="debug">
                    <attr name="class" stringvalue="org.netbeans.modules.perspective.builtin.BuiltInJavaDebugView" />             
                </file>     
            </folder>
            <folder name="custom">
            </folder>
        </folder>
    </folder>
    
    <!--window system config folder-->
    <folder name="Windows2">
    <!--Tc components folder-->
    <folder name="Components">
        <!--Modehack Tc Settings file -->
        <file name="ModeHackTopComponent.settings" url="hacks/ModeHackTopComponentSettings.xml"/>
    </folder>
    <!--Window System Mode Folder-->
    <folder name="Modes">
    <!--Custom javaOutput Mode that use by Java Browsing Perspectie-->
    <file name="javaOutput.wsmode" url="modes/javaOutput.wsmode"/>
    <folder name="javaOutput"/> 
    <!--overwrite default commonpalette mode with custom outline.wsmode config
            see org.netbeans.modules.perspective.modes package
            -->
    <file name="commonpalette.wsmode" url="modes/outline.wsmode"/>
    <folder name="commonpalette"/> 
    <!--overwrite default explorer mode with custom explorer.wsmode config 
            see org.netbeans.modules.perspective.modes package
            -->
    <file name="explorer.wsmode" url="modes/explorer.wsmode"/>
    <folder name="explorer"/> 
    <!--Custom javaExplorer Mode that use by Java Browsing Perspectie and Debug Perspective-->
    <file name="javaExplorer.wsmode" url="modes/javaExplorer.wsmode"/>
    <folder name="javaExplorer"/>
        <!--Custom javaExplorer Mode that use by Java Browsing Perspectie and Debug Perspective-->
        <file name="javaExplorer2.wsmode" url="modes/javaExplorer2.wsmode"/>
        <folder name="javaExplorer2"/>
            
        </folder>    
    </folder>
    
</filesystem>
