/**
 * Replaces SwingUtilities wrapper method calls with the wrapped methods.
 * This provides a negligible performance boost, and makes it more obvious where
 * control is really being transferred.
 */

$su.invokeLater => java.awt.EventQueue.invokeLater :: $su instanceof javax.swing.SwingUtilities;
$su.invokeAndWait => java.awt.EventQueue.invokeAndWait :: $su instanceof javax.swing.SwingUtilities;
$su.isEventDispatchThread() => java.awt.EventQueue.isDispatchThread() :: $su instanceof javax.swing.SwingUtilities;
