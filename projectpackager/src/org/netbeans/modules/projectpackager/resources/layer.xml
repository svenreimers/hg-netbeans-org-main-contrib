<filesystem>
    <folder name="Menu">
        <folder name="File">
            <attr name="Separator2.instance/Import" boolvalue="true" />
            <folder name="Import">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.projectpackager.resources.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/openide/resources/actions/empty.gif"/>
                <file name="org-netbeans-modules-projectpackager-importer-ImportZip.instance"/> 
            </folder>            
            <attr name="Import/Export" boolvalue="true" />
            <folder name="Export">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.projectpackager.resources.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/openide/resources/actions/empty.gif"/>
                <file name="org-netbeans-modules-projectpackager-exporter-ExportZip.instance"/> 
            </folder>
            <attr name="Export/ImporterSeparator1.instance" boolvalue="true" />
            <file name="ImporterSeparator1.instance">
                <attr name="instanceClass" stringvalue="javax.swing.JSeparator" />
            </file>
        </folder>
    </folder>
    
    <folder name="Services">
    	<file name="org-netbeans-modules-projectpackager-resources-ProjectPackagerSettings.settings" url="ProjectPackagerSettings.xml" />
        <folder name="ProjectPackager">
            <file name="export_script.xml" url="export_script.xml"/>                
            <file name="import_script.xml" url="import_script.xml"/>                
        </folder>
    </folder>
    
</filesystem>
